Show # 6 - Links
    LinkedSockets = 6
    Rarity < Unique
    SetTextColor 255 0 0 # TEXTCOLOR: T1 items: currency, 6L, fishing rod
    SetBackgroundColor 255 255 255 # BACKGROUND: T1 Global High Value Drop
    SetBorderColor 255 0 0 # BORDERCOLOR: T1 items: currency, 6L, fishing rod
